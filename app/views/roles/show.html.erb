<div id="block-content">

    <% @role.webtasks.each do |w| %>
      <h2><%= w.taskname %></h2>
      <p><%= w.tasktask %></p>
    <% end %>
  <%= link_to "New Resume", new_resultfile_path, class: "btn btn-primary" %>
  <br />
  <br />

  <table class = "table table-bordered table-striped">
    <thead>.
    <tr>
      <th>Name</th>
      <th>Download Link</th>
      <th> </th>
    </tr>
    </thead>

    <tbody>
    <% @resultfiless=Resultfile.all %>
    <% @resultfiless.each do |res| %>

      <tr>
        <td><%= res.namefile %></td>
        <td><%= link_to "Download Resume", res.namefile_url %></td>
        <td><%= button_to "Delete",  res, method: :delete, class: "btn btn-danger", confirm: "Are you sure that you wish to delete?" %></td>
      </tr>

    <% end %>
    </tbody>

  </table>
</div>
